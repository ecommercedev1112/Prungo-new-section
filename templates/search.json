{
  "sections": {
    "header": {
      "type": "s_search-header",
      "settings": {
        "bg_image_overlay": 30,
        "text_position": "inside",
        "only_results_show": true
      }
    },
    "main": {
      "type": "s_search",
      "blocks": {
        "filters": {
          "type": "filters",
          "settings": {
            "hide_out_of_stock_filter": false,
            "color_swatches": false,
            "collapse_filters": false,
            "show_view_all": true
          }
        }
      },
      "block_order": [
        "filters"
      ],
      "settings": {
        "columns": 4,
        "rows": 3,
        "fallback_collection": "red-light-therapy-panel",
        "pagination_type": "default",
        "show_sort_dropdown": true,
        "show_active_filters": true,
        "sidebar_as_drawer": false,
        "sidebar_width": 20
      }
    }
  },
  "order": [
    "header",
    "main"
  ]
}
